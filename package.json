{
  "name": "whenner",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "npm audit && set CI=true && npm test",
    "eject": "react-scripts eject",
    "package-updates": "npx ncu",
    "package-updates:global": "npx ncu -g",
    "package-updates:install": "npx ncu -u && npm install && npm test"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-big-calendar": "^0.23.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "redux": "^4.0.4",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "24.0.23",
    "@types/moment": "^2.13.0",
    "@types/node": "12.12.17",
    "@types/react": "^16.9.16",
    "@types/react-big-calendar": "^0.22.3",
    "@types/react-dom": "16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-bootstrap": "^0.24.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-immutable-state-invariant": "^2.1.1",
    "husky": "^3.1.0",
    "npm-check-updates": "^4.0.1",
    "stylelint": "^12.0.1",
    "stylelint-config-recommended": "^3.0.0",
    "tslint": "^5.20.1",
    "typescript": "3.7.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test:ci"
    }
  },
  "stylelint": {
    "extends": "stylelint-config-recommended"
  }
}
